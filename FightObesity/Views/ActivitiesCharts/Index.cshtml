@model IEnumerable<FightObesity.Models.ActivitiesChart>

@{
    ViewBag.Title = "Index";
}

<h2>Indoor and Outdoor Activity Examples</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Indoor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Outdoor)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Indoor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Outdoor)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ActivitiesChartID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ActivitiesChartID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ActivitiesChartID })
        </td>
    </tr>
}

</table>
